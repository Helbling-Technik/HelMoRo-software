<launch>

   <master auto="start" />

   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
      <rosparam file="$(find helmoro_2dnav)/param/move_base_params.yaml" command="load" />
      <rosparam file="$(find helmoro_2dnav)/param/common_costmap_params.yaml" command="load" ns="global_costmap" />
      <rosparam file="$(find helmoro_2dnav)/param/common_costmap_params.yaml" command="load" ns="local_costmap" />
      <rosparam file="$(find helmoro_2dnav)/param/local_costmap_params.yaml" command="load" />
      <rosparam file="$(find helmoro_2dnav)/param/global_costmap_params.yaml" command="load" />
      <rosparam file="$(find helmoro_2dnav)/param/global_planner_params.yaml" command="load" />
      <rosparam file="$(find helmoro_2dnav)/param/DWA_local_planner_params.yaml" command="load" />
      <rosparam file="$(find helmoro_2dnav)/param/base_local_planner_params.yaml" command="load" />
   </node>

</launch>